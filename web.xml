<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>ReportingTool2</display-name>
	<welcome-file-list>
		<welcome-file>TrialConfig.html</welcome-file>
	</welcome-file-list>


	<servlet>
		<servlet-name>/NgLoginServlet</servlet-name>
		<servlet-class>com.cts.controller.NgServlet</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>/NgLoginServlet</servlet-name>
		<url-pattern>/NgUser</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>/NgJSONServlet</servlet-name>
		<servlet-class>com.cts.controller.NgArrayServlet</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>/NgJSONServlet</servlet-name>
		<url-pattern>/NgArray</url-pattern>
	</servlet-mapping>

</web-app>